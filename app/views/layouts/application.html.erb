<!DOCTYPE html>
<html>
  <head>
    <title>Artistweb</title>
    <%= csrf_meta_tags %>

    <%#= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag "application", params[:controller], :media => "all" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body background="/assets/taj.jpg">
	<div id="user_nav" style="float: right;">
	    <% if user_signed_in? %>
	        Welcome <%= current_user.email %>. This cannot be cheese?
	        | <%= link_to link_to "Sign out", destroy_user_session_path, :method => :delete %>
	    <% end %>
	</div>
    <%= yield %>
  </body>
</html>
